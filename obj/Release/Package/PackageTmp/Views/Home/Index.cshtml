@model  string
@*todo handle when we have a saved session*@
<script>
    $(document).ready(function () {
        var sessionid = '@ViewData["sessionid"]';
        var sharedSession = '@Html.Raw(Model)';
        if (sharedSession.length) new GROLIAAS.ListDetailsView({sharedSession:sharedSession, sessionId:sessionid}).initComponent();
        else new GROLIAAS.ListSetUpView().initComponent();

    });
</script>
